<main role="main" class="container">
  <div class="starter-template">
    <h1>Hi {{user.name}}</h1>
    <table class = "table">
      <thead>
        <tr>
          <th>ID</th>
          <th>Name</th>
          <th>Email</th>
          <th>Role</th>
        </tr>
      </thead>
      <tbody>
        {{#each user}}
          <tr>
            <td>{{id}}</td>
            <td>{{name}}</td>
            <td>{{email}}</td>
            <td>{{role}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</main>
